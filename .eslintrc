{
  "plugins": [
    "babel",
    "jest"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ]
}

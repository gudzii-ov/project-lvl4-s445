{
  "plugins": [
    "babel"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ]
}
